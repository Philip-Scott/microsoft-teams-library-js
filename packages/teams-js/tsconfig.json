{
  "extends": "../../tsconfig.common.json",
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "declaration": true,
    "declarationMap": true,
    "outDir": "./dist",
    "lib": ["ES5", "DOM", "ES2015.promise"],
    "moduleResolution": "node",
    "noResolve": false,
    "noImplicitAny": false,
    "noEmitOnError": false,
    "noImplicitReturns": true
  },
  "exclude": ["dist", "dts", "node_modules", "./test", "./webpack.config.ts"]
}
